<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:Solution.App.WPF.Styles">

    <!--Colors-->
    <Color x:Key="PurpleBackGround">#FFBF80E2</Color>
    <Color x:Key="WhiteColor">#FFFFFF</Color>

    <!--Brushes-->
    <SolidColorBrush x:Key="PurpleBrush" Color="{StaticResource PurpleBackGround}"/>
    <SolidColorBrush x:Key="WhiteBrush" Color="{StaticResource WhiteColor}"/>

</ResourceDictionary>